<?xml version="1.0" encoding="UTF-8" ?>
<phpunit
    colors="true"
    convertErrorsToExceptions="true"
    convertNoticesToExceptions="true"
    convertWarningsToExceptions="true"
    stopOnFailure="false"
    syntaxCheck="false"
    backupGlobals="false"
    backupStaticAttributes="false">

    <testsuites>
        <testsuite name="Route">
            <directory>./tests</directory>
        </testsuite>
    </testsuites>
</phpunit>